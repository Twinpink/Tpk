while command != "quit":
    command = input("> ").lower()
    if command == "t":
        dice_1 = randrange(1, 7)
        dice_2 = randrange(1, 7)
        dice_3 = randrange(1, 7)
        if dice_1 == 6:
            print(dice_1)
            print(dice_2)
            print(dice_3)
            print("You won 60 points!")
            del dice_1
        elif dice_2 == 6:
            print(dice_1)
            print(dice_2)
            print(dice_3)
            print("You won 60 points!")
            del dice_2
        elif dice_3 == 6:
            print(dice_1)
            print(dice_2)
            print(dice_3)
            print("You won 60 points!")
            del dice_3
